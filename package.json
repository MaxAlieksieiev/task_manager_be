{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend for internet shop",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon src/server.ts",
    "run": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.ts\"",
    "migration:generate": "ts-node node_modules/sequelize-cli/lib/sequelize migration:generate --name",
    "migration:up": "ts-node node_modules/sequelize-cli/lib/sequelize db:migrate",
    "migration:down": "ts-node node_modules/sequelize-cli/lib/sequelize db:migrate:undo",
    "seed:generate": "ts-node node_modules/sequelize-cli/lib/sequelize seed:generate --name",
    "seed:up": "ts-node node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "seed:down": "ts-node node_modules/sequelize-cli/lib/sequelize db:seed:undo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.23",
    "@typescript-eslint/parser": "^5.55.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.3",
    "@types/validator": "^13.7.14",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
